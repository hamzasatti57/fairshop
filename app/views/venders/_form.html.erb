
<%= simple_form_for(@vender) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :city %>
    <%= f.input :address %>
    <%= f.input :gender %>
    <%= f.input :dob %>
    <%= f.input :cnic %>
    <%= f.input :contact %>
    <%= f.input :country %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
