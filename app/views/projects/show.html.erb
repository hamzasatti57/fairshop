              <!-- === product-title === -->

              <h1 class="title" ><%= @project.title %> </h1>

              <div class="clearfix">


                <hr />

                <!-- === info-box === -->

                <div class="info-box">
                  <span><strong>Maifacturer</strong></span>
                  <span><%= @vendor.full_name %></span>
                </div>
                <hr />



